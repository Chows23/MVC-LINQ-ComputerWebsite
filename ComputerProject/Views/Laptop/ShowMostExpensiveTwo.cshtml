
@model List<Laptop>

<h3>
    Show most expensive two laptops.
</h3>


<div>
    @foreach (var laptop in Model)
    {
    <div class="card" style="width: 18rem;">
        <div class="card-body">
            <h5 class="card-title">@laptop.Brand.BrandName</h5>
            <h6 class="card-subtitle mb-2 text-muted">brand name</h6>
            <p class="card-text">The laptop ID: @laptop.Id and Price: @laptop.Price$ .</p>
        </div>
    </div>
    }
</div>
